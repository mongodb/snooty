{
  "name": "snooty",
  "version": "0.6.3",
  "repository": "github:mongodb/snooty",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "auto-changelog": "^1.16.4",
    "babel-jest": "^24.9.0",
    "babel-plugin-emotion": "^10.0.33",
    "babel-preset-gatsby": "^0.2.36",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "husky": "^3.0.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.27",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "puppeteer": "^1.20.0"
  },
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "ensure-master": "node scripts/ensure-master.js",
    "format": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier \"**/*.{js,jsx,json,md}\"",
    "serve": "gatsby serve --prefix-paths",
    "test": "jest",
    "test:regression": "jest regression",
    "test:unit": "jest unit",
    "preversion": "npm run ensure-master && npm run format && npm run lint && npm run test:unit",
    "version": "auto-changelog -p --template keepachangelog && git add CHANGELOG.md",
    "postversion": "git push origin v$npm_package_version && git push origin master"
  },
  "lint-staged": {
    "*.js": [
      "npm run format:fix",
      "npm run lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@leafygreen-ui/badge": "^3.0.2",
    "@leafygreen-ui/box": "^2.1.5",
    "@leafygreen-ui/button": "^7.0.1",
    "@leafygreen-ui/callout": "^1.2.1",
    "@leafygreen-ui/card": "^4.1.0",
    "@leafygreen-ui/checkbox": "^5.0.0",
    "@leafygreen-ui/code": "^7.0.1",
    "@leafygreen-ui/emotion": "^2.0.2",
    "@leafygreen-ui/hooks": "^4.2.1",
    "@leafygreen-ui/icon": "^6.6.0",
    "@leafygreen-ui/icon-button": "^8.0.0",
    "@leafygreen-ui/leafygreen-provider": "^1.1.4",
    "@leafygreen-ui/modal": "^4.1.1",
    "@leafygreen-ui/palette": "^2.0.2",
    "@leafygreen-ui/text-input": "^3.0.4",
    "@leafygreen-ui/tooltip": "^5.0.1",
    "@leafygreen-ui/typography": "^4.3.0",
    "@loadable/component": "^5.13.1",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.24.63",
    "gatsby-plugin-emotion": "^4.3.11",
    "gatsby-plugin-google-tagmanager": "^2.3.15",
    "gatsby-plugin-layout": "^1.3.11",
    "gatsby-plugin-react-helmet": "^3.3.11",
    "html-screen-capture-js": "^1.0.47",
    "mongodb-stitch-browser-sdk": "^4.8.0",
    "mongodb-stitch-server-sdk": "^4.8.0",
    "no-scroll": "^2.1.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.2",
    "rasterizehtml": "^1.3.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-loadable": "^5.5.0",
    "react-responsive": "^8.1.0",
    "sanitize-html": "^1.27.4",
    "style-loader": "^1.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
