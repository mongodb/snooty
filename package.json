{
  "name": "snooty",
  "version": "0.9.8",
  "repository": "github:mongodb/snooty",
  "engines": {
    "node": "^14",
    "npm": "7.*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "auto-changelog": "^2.2.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-emotion": "^10.0.33",
    "babel-preset-gatsby": "^0.5.16",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
