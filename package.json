{
  "name": "snooty",
  "version": "0.1.0",
  "repository": "github:mongodb/snooty",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.1",
    "prettier": "^1.16.3",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": ">=3.1.11"
  },
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop --prefix-paths",
    "lint": "eslint src/ tests/",
    "lintfix": "prettier-eslint --write \"src/**/*.js\" \"tests/*.js\"",
    "serve": "gatsby serve --prefix-paths",
    "test": "jest",
    "test:unit": "jest ./tests/unit/"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^.+\\.(css)$": "<rootDir>/tests/unit/__mockStyles.js"
    },
    "setupFilesAfterEnv": [
      "./src/testsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "prettier-eslint --write \"src/**/*.js\" \"tests/*.js\"",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "axios": "^0.18.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "dotenv": "^7.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-jest": "^22.3.0",
    "express": "^4.16.4",
    "gatsby": "^2.3.4",
    "highlight.js": "^9.15.6",
    "html-webpack-plugin": "^3.2.0",
    "mongodb-stitch-browser-sdk": "^4.0.13",
    "mongodb-stitch-server-sdk": "^4.0.13",
    "prop-types": "^15.7.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-highlight": "^0.12.0",
    "react-loadable": "^5.5.0",
    "start-server-webpack-plugin": "^2.2.5",
    "uuid": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
